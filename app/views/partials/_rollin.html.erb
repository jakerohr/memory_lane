<% p = @partials.find { |i| i[:path] == 'rollin' } %>
<div class="row rollin">
  <div class="col-md-4" data-sr='enter right, flip -90deg, over 3s, reset' >
      <%= image_tag "#{p[:item1]}", class: "img-responsive img-fit" %>
  </div>
  <div class="col-md-4" data-sr='enter right, flip -90deg, over 3s, reset' >
      <%= image_tag "#{p[:item2]}", class: "img-responsive img-fit" %>
  </div>
</div>
