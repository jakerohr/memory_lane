<% p = @partials.find { |i| i[:path] == 'collage' } %>
<div class="row collage">
  <div data-sr='enter left, move 500px, over 3s, reset'class="col-md-4 p1item1">
    <%= image_tag "#{p[:item1]}", class: "img-responsive img-fit" %>
  </div>
  <div data-sr='enter top, move 500px, over 3s, reset'class="col-md-4 p1item2">
    <%= image_tag "#{p[:item2]}", class: "img-responsive img-fit"%>
  </div>
  <div data-sr='enter right, move 500px, over 3s, reset'class="col-md-4 p1item3">
    <%= image_tag "#{p[:item3]}", class: "img-responsive img-fit" %>
  </div>
</div>
<div class="row collage">
  <div data-sr='enter left, move 500px, over 3s, reset'class="col-md-4 p1item4">
    <%= image_tag "#{p[:item4]}", class: "img-responsive img-fit" %>
  </div>
  <div data-sr='wait 3s and then ease in 500px, reset'class="col-md-4">
    <p class="p1item5">
      <%= p[:item5] %>
    </p>
  </div>
  <div data-sr='enter right, move 500px, over 3s, reset'class="col-md-4 p1item6">
    <%= image_tag "#{p[:item6]}", class: "img-responsive img-fit" %>
  </div>
</div>
<div class="row collage">
  <div data-sr='enter left, move 500px, over 3s, reset'class="col-md-4 p1item4">
    <%= image_tag "#{p[:item7]}", class: "img-responsive img-fit" %>
  </div>
  <div data-sr='enter bottom, move 500px, over 3s, reset'class="col-md-4 p1item5">
    <%= image_tag "#{p[:item8]}", class: "img-responsive img-fit" %>
  </div>
  <div data-sr='enter right, move 500px, over 3s, reset'class="col-md-4 p1item6">
    <%= image_tag "#{p[:item9]}", class: "img-responsive img-fit" %>
  </div>
</div>
